#spring.datasource.password=

datasource.default.driver=com.mysql.jdbc.Driver
datasource.default.url=%s:%s://%s:%d/%s
datasource.default.protocol=jdbc
datasource.default.scheme=mysql
datasource.default.host=
datasource.default.name=
datasource.default.user=
datasource.default.pass=


spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL5InnoDBDialect
spring.jpa.hibernate.naming-strategy=net.rainmore.platform.core.helpers.jpa.JpaCustomNamingStrategy
# don't put it to "validate" as hibernate complain the mapping between mysql bool (tinyint) and Boolean
spring.jpa.hibernate.ddl-auto=none
spring.jpa.show-sql=true
spring.jpa.generate-ddl=false

error.whitelabel.enabled=false

